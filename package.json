{
	"engines": {
		"node": "18.x"
	},
	"name": "tarkov-task",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write --plugin-search-dir=. .",
		"postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/; cp prisma/schema.prisma .vercel_build_output/functions/node/render/; ls .vercel_build_output/functions/node/render/"
	},
	"devDependencies": {
		"@fouita/tooltip": "^0.0.1",
		"@playwright/test": "^1.22.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.71",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.7.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-loading-spinners": "^0.1.7",
		"svelte-preprocess": "^4.10.6",
		"svelte-tooltip": "^1.2.0",
		"tailwindcss": "^3.1.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@prisma/client": "^4.7.0",
		"@supabase/supabase-js": "^1.35.4",
		"cookie": "^0.4.1",
		"daisyui": "^2.17.0",
		"dotenv": "^16.0.1",
		"graphql": "^16.5.0",
		"graphql-request": "^4.3.0"
	}
}
